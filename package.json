{
  "name": "gocd-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "node_modules/.bin/eslint server/app.js",
    "start": "node_modules/.bin/babel-node ./bin/appServer",
    "start-dev": "node_modules/.bin/babel-node ./bin/devServer",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore assets/js",
    "add-flexbox": "cp node_modules/flexboxgrid/css/index.css assets/stylesheets/flexbox.css",
    "add-weather-icons": "cp node_modules/weather-icons/css/weather-icons.css assets/stylesheets/weather-icons.css && cp -r node_modules/weather-icons/font/ assets/font"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "chart.js": "^1.0.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "flexboxgrid": "^6.3.0",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-chartjs": "^0.7.3",
    "react-dom": "^0.14.7",
    "react-tap-event-plugin": "^0.2.2",
    "request": "^2.69.0",
    "serve-favicon": "~2.3.0",
    "weather-icons": "^1.3.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "vars-on-top": 2,
      "no-undef": 2
    }
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "ejs": "^2.4.1",
    "eslint": "^1.10.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
