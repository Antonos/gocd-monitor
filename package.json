{
  "name": "gocd-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "node_modules/.bin/eslint server/app.js",
    "start": "node_modules/.bin/babel-node ./bin/www",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore assets/js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "serve-favicon": "~2.3.0",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-tap-event-plugin": "^0.2.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "vars-on-top": 2,
      "no-undef": 2
    }
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "ejs": "^2.4.1",
    "eslint": "^1.10.3",
    "webpack": "^1.12.13"
  }
}
